{
    "name": "@sap/guided-answers-extension-webapp",
    "description": "Guided Answers extension webview module",
    "version": "1.7.0",
    "author": "SAP SE",
    "main": "dist/index.js",
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "build": "pnpm clean && pnpm build:index && pnpm build:webapp",
        "clean": "rimraf ./dist",
        "build:index": "tsc --pretty --project ./tsconfig.json --emitDeclarationOnly",
        "build:webapp": "node esbuild.js",
        "format": "prettier --write '**/*.{js,json,ts,tsx,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "node esbuild.js --watch",
        "test": "jest --maxWorkers=2 --silent"
    },
    "devDependencies": {
        "@reduxjs/toolkit": "1.8.6",
        "@sap/guided-answers-extension-types": "workspace:*",
        "@types/react": "17.0.43",
        "@types/react-dom": "17.0.14",
        "@types/react-redux": "7.1.24",
        "@vscode/webview-ui-toolkit": "1.1.0",
        "autoprefixer": "10.4.13",
        "esbuild": "0.15.12",
        "esbuild-css-modules-plugin": "2.6.2",
        "esbuild-plugin-replace": "1.3.0",
        "esbuild-plugin-svgr": "1.0.1",
        "esbuild-sass-plugin": "2.4.0",
        "html-react-parser": "3.0.4",
        "i18next": "21.6.14",
        "jest-css-modules-transform": "4.4.2",
        "jest-environment-jsdom": "28.1.2",
        "path": "0.12.7",
        "postcss": "8.4.18",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-i18next": "11.10.0",
        "react-icons": "4.6.0",
        "react-redux": "7.2.8",
        "redux": "4.2.0",
        "@testing-library/react": "12.1.5",
        "@testing-library/dom": "8.19.1",
        "redux-mock-store": "1.5.4",
        "@types/redux-mock-store": "1.0.3",
        "uuid": "3.4.0"
    },
    "dependencies": {
        "@fluentui/react": "8.99.2",
        "@fluentui/react-focus": "8.8.8",
        "@fluentui/style-utilities": "8.8.1",
        "@fluentui/utilities": "8.13.2",
        "@sap-ux/ui-components": "1.0.16",
        "react-infinite-scroll-component": "6.1.0",
        "@testing-library/jest-dom": "5.16.5",
        "@types/testing-library__jest-dom": "5.14.5"
    }
}
