{
    "name": "@sap/guided-answers-extension-webapp",
    "description": "Guided Answers extension webview module",
    "version": "1.1.0",
    "author": "SAP SE",
    "main": "dist/index.js",
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "build": "pnpm clean && pnpm build:index && pnpm build:webapp",
        "clean": "rimraf ./dist",
        "build:index": "tsc --pretty --project ./tsconfig.json --emitDeclarationOnly",
        "build:webapp": "node esbuild.js",
        "format": "prettier --write '**/*.{js,json,ts,tsx,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "node esbuild.js --watch",
        "test": "jest --maxWorkers=2 --silent"
    },
    "devDependencies": {
        "@reduxjs/toolkit": "1.6.1",
        "@sap/guided-answers-extension-types": "workspace:*",
        "@types/enzyme": "3.10.12",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/react": "17.0.43",
        "@types/react-dom": "17.0.14",
        "@types/react-redux": "7.1.23",
        "@vscode/webview-ui-toolkit": "1.0.0",
        "autoprefixer": "10.4.4",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "esbuild": "0.14.39",
        "esbuild-css-modules-plugin": "2.2.5",
        "esbuild-plugin-replace": "1.2.0",
        "esbuild-plugin-svgr": "1.0.1",
        "esbuild-sass-plugin": "2.2.4",
        "html-react-parser": "1.4.8",
        "i18next": "21.6.14",
        "jest-css-modules-transform": "4.4.2",
        "jest-environment-jsdom": "28.1.2",
        "path": "0.12.7",
        "postcss": "8.4.12",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-i18next": "11.10.0",
        "react-icons": "4.3.1",
        "react-redux": "7.2.8",
        "redux": "4.0.5"
    },
    "dependencies": {
        "@fluentui/react": "8.80.0",
        "@fluentui/react-focus": "8.7.3",
        "@fluentui/style-utilities": "8.7.4",
        "@fluentui/utilities": "8.10.1",
        "@sap-ux/ui-components": "1.0.5",
        "react-infinite-scroll-component": "6.1.0"
    }
}
