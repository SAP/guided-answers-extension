{
    "name": "@sap/guided-answers-extension-webapp",
    "description": "Guided Answers extension webview module",
    "version": "1.27.0",
    "author": "SAP SE",
    "main": "dist/index.js",
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "build": "pnpm clean && pnpm build:index && pnpm build:webapp",
        "clean": "rimraf --glob ./dist",
        "build:index": "tsc --pretty --project ./tsconfig.json --emitDeclarationOnly",
        "build:webapp": "node esbuild.js",
        "format": "prettier --write '**/*.{js,json,ts,tsx,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "node esbuild.js --watch",
        "test": "jest --maxWorkers=2 --silent"
    },
    "devDependencies": {
        "@reduxjs/toolkit": "1.9.5",
        "@sap/guided-answers-extension-types": "workspace:*",
        "@testing-library/dom": "8.20.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "12.1.5",
        "@types/react": "17.0.62",
        "@types/react-copy-to-clipboard": "5.0.4",
        "@types/react-dom": "17.0.20",
        "@types/react-redux": "7.1.25",
        "@types/redux-mock-store": "1.0.3",
        "@types/testing-library__jest-dom": "5.14.6",
        "autoprefixer": "10.4.14",
        "esbuild": "0.18.10",
        "esbuild-css-modules-plugin": "2.7.1",
        "esbuild-plugin-replace": "1.4.0",
        "esbuild-plugin-svgr": "1.1.0",
        "esbuild-sass-plugin": "2.10.0",
        "html-react-parser": "4.0.0",
        "i18next": "23.2.6",
        "jest-css-modules-transform": "4.4.2",
        "jest-environment-jsdom": "29.5.0",
        "path": "0.12.7",
        "postcss": "8.4.24",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-i18next": "13.0.1",
        "react-icons": "4.10.1",
        "react-redux": "8.1.1",
        "redux": "4.2.1",
        "redux-mock-store": "1.5.4",
        "uuid": "9.0.0"
    },
    "dependencies": {
        "@fluentui/react": "8.110.7",
        "@fluentui/react-focus": "8.8.30",
        "@fluentui/style-utilities": "8.9.16",
        "@fluentui/utilities": "8.13.18",
        "@sap-ux/ui-components": "1.8.3",
